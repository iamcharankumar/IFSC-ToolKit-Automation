		<dependency>
			<groupId>com.jayway.restassured</groupId>
			<artifactId>rest-assured</artifactId>
			<version>1.6</version>
			<scope>test</scope>
		</dependency>

		<!-- The HttpClient component supports the client-side of RFC 1945 (HTTP/1.0) 
